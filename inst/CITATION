citHeader("To cite list in publications use:")

citEntry(entry = "misc",
  title        = “{redist}: Markov Chain Monte Carlo Methods for Redistricting Simulation",
  author       = personList(as.person(“Ben Fifield“), as.person(“Alexander Tarr“), as.person(“Michael Higgins”),
                   as.person("Kosuke Imai")),
  howpublished = "Available at The Comprehensive R Archive Network (CRAN)",
  year         = "2015",
  url          = "http://CRAN.R-project.org/package=redist”,

  textVersion  =
  paste(“Ben Fifield, Alexander Tarr, Michael Higgins, and Kosuke Imai (2015).",
        “redist: Markov Chain Monte Carlo Methods for Redistricting Simulation.”,
        "Available at The Comprehensive R Archive Network (CRAN).",
        "URL http://CRAN.R-project.org/package=redist.”)
)